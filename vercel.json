{
  "buildCommand": "npm run build",
  "devCommand": "npm run start",
  "installCommand": "npm install",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Expires",
          "value": "Wed, 3 Oct 2024 07:00:00 GMT"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Content-Language",
          "value": "fr"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [{ "key": "Expires", "value": "Wed, 3 Oct 2024 07:00:00 GMT" }]
    }
  ]
}
